{
  "name": "@mistong/eui-dialog",
  "version": "0.1.4",
  "description": "eui-dialog component",
  "main": "dist/index.js",
  "scripts": {
    "start": "eui server",
    "build": "eui build",
    "test": "eui test",
    "coverage": "eui test --coverage",
    "lint": "eui lint",
    "lint:fix": "eui lint --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "cz": "git-cz",
    "precommit": "lint-staged && npm run test",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build"
  },
  "contributors": [
    "mistong eui"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MST-EUI/eui-dialog.git"
  },
  "bugs": {
    "url": "https://github.com/MST-EUI/eui-dialog/issues"
  },
  "homepage": "https://github.com/MST-EUI/eui-dialog",
  "keywords": [
    "react",
    "react-component",
    "eui-dialog",
    "eui"
  ],
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@mistong/eui-cli": "^0.1.17",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-14": "^1.0.5",
    "es5-shim": "^4.5.10",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "object-assign": "^4.1.1",
    "react": "^0.14.9",
    "react-dom": "^0.14.9",
    "react-addons-test-utils": "^0.14.0",
    "react-test-renderer": "^16.4.1"
  },
  "dependencies": {
    "@mistong/eui-button": "^0.1.1",
    "@mistong/eui-css": "^0.1.2",
    "@mistong/eui-icon": "^0.1.1",
    "classnames": "^2.2.6",
    "rc-dialog": "^7.1.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "eui lint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ]
    }
  }
}
